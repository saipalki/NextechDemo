{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ServiceUrls": {
    "NewsProvidersApi": "https://hacker-news.firebaseio.com/v0/"
  },
  "AllowedHosts": "*",
  "CacheOptions": {
    "AbsoluteExpiration": 7200, // second 60 * 2 * 60
    "SlidingExpiration": 3600, // second 60 * 1 * 60
    "Priority": "High" // Low, Normal, High, NeverRemove
  }
}
